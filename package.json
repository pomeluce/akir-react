  "scripts": {
    "main:dev": "vite --mode development -- --module=main",
    "main:pro": "vite --mode production -- --module=main",
    "tauri:dev": "vite --mode development -- --module=tauri",
    "tauri:pro": "vite --mode production -- --module=tauri",
    "build:dev": "tsc && node vite.build.js development",
    "build:pro": "tsc && node vite.build.js production",
    "build:main:dev": "tsc && vite build --mode development -- --module=main",
    "build:main:pro": "tsc && vite build --mode development -- --module=main",
    "build:tauri:dev": "tsc && vite build --mode development -- --module=tauri",
    "build:tauri:pro": "tsc && vite build --mode production -- --module=tauri",
    "preview:main": "vite preview -- module=main",
    "preview:tauri": "vite preview -- --module=tauri",
    "tauri-dev": "cargo tauri dev",
    "tauri-build": "cargo tauri build"
  },
