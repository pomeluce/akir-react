  "scripts": {
    "dev": "vite --mode development",
    "pro": "vite --mode production",
    "build:dev": "tsc && vite build --mode development",
    "build:pro": "tsc && vite build --mode production",
    "build:all:dev": "tsc && node vite.build.js development",
    "build:all:pro": "tsc && node vite.build.js production",
    "preview": "vite preview",
    "tauri:dev": "vite --mode development -- --module=tauri",
    "tauri:pro": "vite --mode production -- --module=tauri",
    "tauri:build:dev": "tsc && vite build --mode development -- --module=tauri",
    "tauri:build:pro": "tsc && vite build --mode production -- --module=tauri",
    "tauri:preview": "vite preview -- --module=tauri",
    "tauri-dev": "cargo tauri dev",
    "tauri-build": "cargo tauri build"
  },
