{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "org.pomeluce.rapidify-react",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "webview:default",
    "app:default",
    "resources:default",
    "image:default",
    "menu:default",
    "tray:default",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:allow-message",
    "shell:allow-open"
  ]
}
